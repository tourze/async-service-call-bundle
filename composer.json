{
  "name": "tourze/async-command-bundle",
  "description": "异步执行服务方法",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/common": "^3.5",
    "doctrine/orm": "^3.0",
    "psr/container": "^1.1|^2.0",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/serializer": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/async-contracts": "0.0.*",
    "tourze/symfony-async-bundle": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "symfony/phpunit-bridge": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\AsyncServiceCallBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\AsyncServiceCallBundle\\Tests\\": "tests"
    }
